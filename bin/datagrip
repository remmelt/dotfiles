#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10685 at 2021-11-15T15:31:07.231153

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/remmelt/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-1/212.5457.41/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"