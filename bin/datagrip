#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9547 at 2021-07-16T14:14:51.953074

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/remmelt/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/211.7142.37/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"