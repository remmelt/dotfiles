#!/bin/bash
#Generated by JetBrains Toolbox 2.0.4.17212 at 2023-09-18T12:57:39.323270

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/remmelt/Applications/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${intellij_args[@]}"