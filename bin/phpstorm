#!/bin/sh
#Generated by JetBrains Toolbox 1.20.7940 at 2021-02-17T14:43:15.660025

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/remmelt/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm-ARM/ch-0/203.7148.74/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"