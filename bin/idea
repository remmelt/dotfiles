#!/bin/sh
#Generated by JetBrains Toolbox 1.20.7940 at 2021-02-11T21:22:43.263863

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/remmelt/Library/Application Support/JetBrains/Toolbox/apps/IDEA-ARM/ch-0/203.7148.57/IntelliJ IDEA.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"